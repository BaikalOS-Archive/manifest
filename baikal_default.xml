<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="baikalos"
           fetch="https://github.com/"
           review="gerrit.baikalos.org"
           revision="refs/heads/pi" />

  <remote  name="lineage"
           fetch="https://github.com/LineageOS"
           review="review.lineageos.org"
           revision="refs/heads/lineage-16.0" />

  <!-- Remove unused projects -->
  <remove-project name="manifest" />
  <!-- remove-project name="bionic" /-->
  <remove-project name="build" />
  <remove-project name="build_soong" />
  <!-- remove-project name="device_gzosp_sepolicy" />
  <remove-project name="device_qcom_sepolicy" />
  <remove-project name="frameworks_av" /-->
  <remove-project name="frameworks_base" />
  <!-- remove-project name="frameworks_native" />
  <remove-project name="hardware_interfaces" />
  <remove-project name="packages_apps_Dialer" / -->
  <remove-project name="packages_apps_Settings" />
  <remove-project name="platform/packages/services/Telephony" />
  <remove-project name="system_core" />
  <remove-project name="vendor_gzosp" />


  <!-- Baikal repositories -->
  <project path="platform_manifest" name="baikalos/manifest" remote="baikalos" />
  <project path="build/make" name="baikalos/build" remote="baikalos" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="baikalos/build_soong"  remote="baikalos" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <!-- project path="bionic" name="baikalos/bionic" remote="baikalos" />
  <project path="device/baikalos/sepolicy" name="baikalos/device_baikalos_sepolicy" remote="baikalos" />
  <project path="device/qcom/sepolicy" name="baikalos/device_qcom_sepolicy" remote="baikalos" />
  <project path="device/qcom/sepolicy-legacy" name="baikalos/device_qcom_sepolicy-legacy" remote="baikalos" />
  <project path="frameworks/av" name="baikalos/frameworks_av" remote="baikalos" groups="pdk" / -->
  <project path="frameworks/base" name="baikalos/frameworks_base" remote="baikalos" groups="pdk-cw-fs,pdk-fs" />
  <!-- project path="frameworks/native" name="baikalos/frameworks_native" remote="baikalos" groups="pdk" />-->
  <project path="frameworks/opt/baikalosgear" name="baikalos/frameworks_opt_baikalosgear" remote="baikalos" />
  <!--<project path="hardware/interfaces" name="baikalos/hardware_interfaces" groups="pdk" remote="baikalos" / -->
  <project path="packages/apps/BaikalDelta" name="baikalos/android_packages_apps_BaikalDelta" remote="baikalos" />
  <project path="packages/apps/BaikalOSExtras" name="baikalos/packages_apps_BaikalOSExtras" remote="baikalos" />
  <!--<project path="packages/apps/Dialer" name="baikalos/packages_apps_Dialer" remote="baikalos" / -->
  <project path="packages/apps/Settings" name="baikalos/packages_apps_Settings" remote="baikalos" groups="pdk-fs" />
  <!-- project path="packages/apps/SnapdragonCamera" name="baikalos/packages_apps_SnapdragonCamera" remote="baikalos" /-->
  <project path="packages/overlays/BaikalOS" name="baikalos/packages_overlays_BaikalOS" remote="baikalos" />
  <project path="packages/services/Telephony" name="baikalos/packages_services_Telephony" remote="baikalos" />
  <project path="system/core" name="baikalos/system_core" remote="baikalos" groups="pdk" />
  <project path="vendor/baikalos" name="baikalos/vendor_baikalos" remote="baikalos" />

</manifest>
